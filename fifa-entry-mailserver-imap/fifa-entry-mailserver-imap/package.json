{
  "name": "fifa-entry-automation",
  "version": "1.0.0",
  "description": "Dolphin Anty automation script for FIFA entry",
  "main": "main.ts",
  "bin": "main.ts",
  "scripts": {
    "start": "ts-node main.ts",
    "dev": "ts-node main.ts",
    "test-proxies": "ts-node test-proxy-speeds.ts",
    "build": "tsc && pkg dist/main.js --out-path dist --public",
    "build-win": "tsc && pkg dist/main.js --targets node18-win-x64 --out-path dist --public",
    "build-mac": "tsc && pkg dist/main.js --targets node18-macos-x64 --out-path dist --public",
    "build-linux": "tsc && pkg dist/main.js --targets node18-linux-x64 --out-path dist --public",
    "build-all": "tsc && pkg dist/main.js --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --out-path dist --public",
    "release": "npm run build-all && node build-release.js",
    "compile": "tsc",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "canvas": "^2.11.2",
    "dotenv": "^17.2.2",
    "ghost-cursor": "^1.4.1",
    "imap": "^0.8.19",
    "imapflow": "^1.0.195",
    "mailparser": "^3.6.5",
    "node-geocoder": "^4.4.1",
    "playwright": "^1.55.0",
    "puppeteer-core": "^21.0.0",
    "random-js": "^2.1.0",
    "random-location": "^1.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "dolphin-anty",
    "automation",
    "puppeteer",
    "fifa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/node": "^20.0.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "pkg": {
    "assets": [
      "dist/helpers/**/*",
      "dist/modules/**/*",
      "settings.txt",
      "information.csv",
      "proxies.txt"
    ],
    "scripts": [
      "dist/main.js",
      "dist/helpers/**/*.js",
      "dist/modules/**/*.js"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linux-x64"
    ]
  }
}
